<div class="payment-success payment-step">
  <div class="direction">
    <a class="wc-back-nav" onclick="javascript:history.back();"><i class="fa fa-angle-left"></i> So let's go back to the
      list</a>
  </div>
</div>

<div class="wc-panel">
  <div class="person_img">
    <img class="img-fluid" src="/themes/custom/wristcheck_barrio/images/personstep.jpg">
  </div>
  <div class="row wc-sell-person">
    <div class="col-md-6">
      <div class="person_info">
        <div class="wc-panel3__title">THE PERSONAL DATA</div>
        {{ drupal_entity('webform','sell_person_info') }}
        <div class="birth">
          <label class="">Date of birth</label>
          <input id="datepicker" placeholder="day/month/year" type="text">
        </div>
      </div>
      <div class="address_info">
        <div class="wc-panel3__title">ADDRESS INFORMATION</div>
        {{ drupal_entity('webform','sell_person_address') }}
      </div>
      <div class="person_des">
        <div class="wc-panel__sub">Your information will be handled securely</div>
        <p>We strictly abide by the relevant privacy protection regulations.</p>
        <p>Anything you write in WRISTCHECK will be transmitted entrypted (SSL) over a secure
          network connection (HTTPS).</p>
        <p>Your personal information is registered and sold on wristcheck only and will not be disclosed to the
          public</p>
      </div>
      <div class="text-right mt-100">
        <button type="button" class="btn btn-default btn-lg wc-btn-dark">
          <div class="wc-btn-cont">
            <span class="fa  fa-arrow-right"></span>
            {{ '| CONTINUE'|t }}
          </div>
        </button>
      </div>
    </div>
    <div class="col-md-6">
      <div class="wc-sell-watch-input-right-box">
        <div class="wc-sell-watch-input-img-box"></div>
        <label>title</label>
        <input readonly class="watch_title" placeholder="watch title"/>
        <label>price</label>
        <input readonly class="watch_price" placeholder="Ask price 0.00"/>
      </div>
    </div>
  </div>
</div>

