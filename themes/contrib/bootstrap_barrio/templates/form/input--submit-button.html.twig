{#
/**
 * @file
 * Theme override for an 'input' #type form element.
 *
 * Available variables:
 * - attributes: A list of HTML attributes for the input element.
 * - children: Optional additional rendered elements.
 *
 * @see template_preprocess_input()
 */
#}
{% if(attributes.value == 'Remove') %}
<button{{ attributes.addClass('wc-btn-dark wc-btn-large') }} data-progress-type="WCFullprogress" >
    <span class="wc-btn-cont">
        {{ safe_value_label | raw }}
    </span>
</button>
{% else %}
<button{{ attributes.addClass('wc-btn-dark wc-btn-large wc-form-submit') }} data-progress-type="WCFullprogress" >
    <span class="wc-btn-cont">
        {{ safe_value_label | raw }}
    </span>
</button>
{% endif %}
{{ children }}
