{% elseif id == '_portfolio_view_'%}
    {% if display_id == 'por_section' %}
        <section id="portfolio-single" class="portfolio portfolio-section portfolio-single portfolio-single-4 portfolio-single-7">
          <div class="container">
            <div class="row">
              <div class="col-sm-12 col-md-12 col-lg-12">
                <div class="portfolio-related portfolio-animation">
                  <div class="row">
                    {{ header }}
                    {{ rows }}
                  </div>
                  <!-- .row end -->
                </div>
                <!-- .portfolio-related end -->
              </div>
            </div>
            <!-- .row end -->
          </div>
          <!-- .container end -->
        </section>
    {% elseif display_id == 'attachment_1' %}
        {{ header }}
    {% elseif display_id == 'portfolio_vintage' %}
        {% set nids = rows|render|striptags|trim|trim(',')|explode(',') %}
        <section id="portfolio-masonry" class="portfolio portfolio-vintage portfolio-4col  portfolio-animation bg-gray pb-40">
            <div class="container">
                <div class="row">
                    <!-- Portfolio Filter
                        ==========================  =================== -->
                    {{ footer }}
                    {{ header }}
                    <!-- .Portfolio-filter end -->
                </div>
            </div>
            <!-- .container end -->
            <div class="container">
                <div id="portfolio-all" class="row">
                    <!-- Portfolio Item #1 -->
                    {% for nid in nids %}
                        {% set node = nid|trim|node_load %}
                        {% set category = '' %}
                        {% set category_name = '' %}
                        {% for term in node.field_categories  %}
                          {% set cat = term.target_id|term_load %}
                          {% set category_name = category_name ~ cat.getName ~ ' / ' %}
                          {% set temp = "filter-" ~ term.target_id %}
                          {% set category = category ~ temp ~ " "  %}
                        {% endfor %}
                        {% set img_uri = node.field_images|first.entity.uri.value %}
                        {% set image340x414 = getUrlByImageStyle(img_uri, 'image340x414') %}
                        {% set img_url = file_url(img_uri) %}
                        <div class="col-sm-6 col-md-6 col-lg-3 portfolio-item {{ category }}">
                            <div class="portfolio-item-box">
                                <div class="portfolio-img">
                                    <img src="{{ image340x414 }}" alt="Portfolio Item">
                                    <div class="portfolio-hover">
                                        <div class="portfolio-hover-content">
                                            <div class="portfolio-meta">
                                                <div class="portfolio-title">
                                                    <h4><a href="{{ path('entity.node.canonical', {'node': node.id}) }}">{{ node.title.value }}</a></h4>
                                                </div>
                                                <div class="portfolio-cat">
                                                    <span>{{ category_name|trim(' ')|trim('/') }}</span>
                                                </div>
                                            </div>
                                            <!-- .Portfolio-meta end -->
                                            <div class="portfolio-action">
                                                <div class="portfolio-zoom-border">
                                                    <a class="img-popup zoom" href="{{ img_url }}" title="{{ node.title.value }}">
                                                    <i class="wristcheck-zoom"></i>
                                                    </a>
                                                </div>
                                            </div>
                                            <!-- .Portfolio-action end -->
                                        </div>
                                        <!-- .portfolio-hover-content end -->
                                    </div>
                                    <!-- .Portfolio-hover end -->
                                </div>
                                <!-- .Portfolio-img end -->
                            </div>
                            <!-- .Portfolio-item-box end -->
                        </div>
                    {% endfor %}
                </div>
                <!-- .row end -->
            </div>
        </section>
    {% elseif display_id == 'portfolio_masory_space' %}
        {% set nids = rows|render|striptags|trim|trim(',')|explode(',') %}
        <section id="portfolio-masonry" class="portfolio portfolio-masonry masonry-spaced portfolio-4col portfolio-animation">
            <div class="container">
                <div class="row heading heading-2 mb-70">
                   {{ footer }}
                </div>
                <div class="row">
                    <!-- Portfolio Filter
                        ============================================= -->
                    {{ header }}
                    <!-- .Portfolio-filter end -->
                </div>
            </div>
            <!-- .container end -->
            <div class="container">
                <div id="portfolio-all" class="row">
                    {% for nid in nids %}
                        {% set node = nid|trim|node_load %}
                        {% set category = '' %}
                        {% set category_name = '' %}
                        {% for term in node.field_categories  %}
                          {% set cat = term.target_id|term_load %}
                          {% set category_name = category_name ~ cat.getName ~ ' / ' %}
                          {% set temp = "filter-" ~ term.target_id %}
                          {% set category = category ~ temp ~ " "  %}
                        {% endfor %}
                        {% set img_uri = node.field_images|first.entity.uri.value %}
                        {% set image350x291 = getUrlByImageStyle(img_uri, 'image350x291') %}
                        {% set image350x501 = getUrlByImageStyle(img_uri, 'image350x501') %}
                        {% set image350x361 = getUrlByImageStyle(img_uri, 'image350x361') %}
                        {% set image350x571 = getUrlByImageStyle(img_uri, 'image350x571') %}
                        {% set img_url = file_url(img_uri) %}
                        <!-- Portfolio Item #1 -->
                        <div class="col-sm-12 col-md-6 col-lg-3 portfolio-item {{ category }}">
                            <div class="portfolio-img">
                                {% if loop.index % 8  == 1 or loop.index % 8 ==  0%}
                                    <img src="{{ image350x291 }}" alt="Portfolio Item">
                                {% elseif loop.index % 8 == 2 or loop.index % 8 == 6 %}
                                    <img src="{{ image350x501 }}" alt="Portfolio Item">
                                {% elseif loop.index % 8 == 3 or loop.index % 8 == 7 %}
                                    <img src="{{ image350x361 }}" alt="Portfolio Item">
                                {% elseif loop.index % 8 == 4 or loop.index % 8 == 5 %}
                                    <img src="{{ image350x571 }}" alt="Portfolio Item">
                                {% endif %}
                                <div class="portfolio-hover">
                                    <div class="portfolio-hover-content">
                                        <div class="portfolio-meta">
                                            <div class="portfolio-title">
                                                <h4><a href="{{ path('entity.node.canonical', {'node': node.id}) }}">{{ node.title.value }}</a></h4>
                                            </div>
                                            <div class="portfolio-cat">
                                                {{ category_name|trim(' ')|trim('/') }}
                                            </div>
                                        </div>
                                        <!-- .Portfolio-meta end -->
                                        <div class="portfolio-action">
                                            <div class="portfolio-zoom">
                                                <a class="{{ img_url }}" title="{{ node.title.value }}">
                                                <i class="wristcheck-zoom"></i>
                                                </a>
                                            </div>
                                        </div>
                                        <!-- .Portfolio-action end -->
