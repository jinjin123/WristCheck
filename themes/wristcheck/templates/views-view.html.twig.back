{% if id == 'product_view' %}
    {% if display_id == 'product_relative' %}
        <div class="product-related shop-categories mt-0">
            {{ header }}
            <div class="row">
                {{ rows }}
            </div><!-- .row end -->
        </div>
    {% elseif display_id == 'product_categories' %}
        <div class="row">
            {{ rows }}
        </div>
    {% elseif display_id == 'product_sidebar' %}
        <div class="widget widget-recent-products">
            {{ header }}
            <div class="widget-content">
                {{ rows }}
            </div><!-- .widget-content end -->
        </div>  
    {% elseif display_id == 'product_content' %} 
        <div class='row'>
            {{ rows }}
        </div>
    {% elseif display_id == 'product_search' %} 
        <div class='row'>
            {{ rows }}
        </div>
    {% elseif display_id == 'page_filter' %} 
        <div class='row'>
            {{ rows }}
        </div>
    {% else %}
        {%
        set classes = [
            dom_id ? 'js-view-dom-id-' ~ dom_id,
        ]
        %}
            {{ exposed }}
        <div{{ attributes.addClass(classes) }}>
            {# {{ title_prefix }} #}
            {{ title }}
            <!-- {{ title_suffix }}  -->
            {% if header %}     
                {{ header }}
            {% endif %}
            {{ attachment_before }}
            {% if rows %}  
                {{ rows }}
            {% endif %}
            {% if footer %}
                {{ footer }}
            {% endif %}
            {% if pager %}
            
                {{ pager }}
           
            {% endif %}
            {{ attachment_after }}
            {{ more }}
            {{ feed_icons }}
        </div>
    {% endif %}
